    },
    "icons": {
        "outline": "assets/outline.png",
        "color": "assets/color.png"
    },
    "accentColor": "#230201",
    "localizationInfo": {
        "defaultLanguageTag": "en-us",
        "additionalLanguages": []
    },
    "authorization": {
        "permissions": {
            "resourceSpecific": [
                {
                    "name": "Mailbox.ReadWrite.User",
                    "type": "Delegated"
                }
            ]
        }
    },
    "webApplicationInfo": {
        "id": "9e771e21-8974-435d-aa55-a7c6a69f8137",
        "resource": "api://9e771e21-8974-435d-aa55-a7c6a69f8137"
    },
    "validDomains": [
        "localhost",
        "contoso.com"
    ],
    "extensions": [
        {
            "requirements": {
                "scopes": [
                    "mail"
                ],
                "capabilities": [
                    {
                        "name": "Mailbox",
                        "minVersion": "1.3"
                    }
                ]
            },
            "runtimes": [
                {
                    "id": "DefaultTaskPaneRuntime",
                    "type": "general",
                    "code": {
                        "page": "https://localhost:{PORT}/taskpane.html"
                    },
                    "lifetime": "short",
                    "actions": [
            {
                "id": "TaskPaneRuntimeShow",
                "type": "openPage",
                "pinnable": false,
                "view": "dashboard",
                "supportsNoItemContext": true,
                "multiselect": true
            }
        ]
                   

                },
                {
                    "id": "ComposeTaskPaneRuntime",
                    "type": "general",
                    "code": {
                        "page": "https://localhost:{PORT}/msgcompose.html"
                    },
                    "lifetime": "short",
                    "actions": [
            {
                "id": "TaskPaneRuntimeShow",
